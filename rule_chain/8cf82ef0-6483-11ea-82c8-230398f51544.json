      },
      "configuration" : {
        "jsScript" : "var newMsg;\nvar newMetadata;\n\nvar reportingFreq = 60 * parseInt(msg.tempRHReportingFreq);\n\nif (metadata.shared_devTempThresholdsEnabled === true)\nTTEnabled = 1;\nelse TTEnabled = 0;\n\nif (metadata.shared_devRHThresholdsEnabled === true)\nRHTEnabled = 1;\nelse RHTEnabled = 0;\n\nnewMsg = {\n\"MsgType\":3,\n\"Options\":0,\n\"tempHigh\":parseInt(metadata.shared_devTempHigh),\n\"tempLow\":parseInt(metadata.shared_devTempLow),\n\"tempThresholdsEnabled\":TTEnabled,\n\"RHThresholdsEnabled\":RHTEnabled,\n\"RHHigh\":parseInt(metadata.shared_devRHHigh),\n\"RHLow\":parseInt(metadata.shared_devRHLow),\n\"reportingFreq\":reportingFreq,\n\"batteryType\":parseInt(metadata.shared_batteryType),\n\"sensorAggregate\":parseInt(metadata.shared_sensorAggregate),\n\"LED_BLE\":parseInt(metadata.shared_LED_BLE),\n\"LED_LoRa\":parseInt(metadata.shared_LED_LoRa)\n};\n\nnewMetadata = {\n    \"devId\":metadata.originatorName\n};\nreturn {msg: newMsg, metadata: newMetadata, msgType: msgType};"
