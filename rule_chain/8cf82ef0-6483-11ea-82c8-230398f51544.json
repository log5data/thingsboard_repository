      "configuration" : {
        "jsScript" : "var newMsg;\nvar newMetadata;\nvar sensor = 'none';\nif (msg.pressureReportingFreq)\n   sensor = 'airpressure'; \nif (msg.dustReportingFreq)\n    sensor = 'dust';\nif (msg.tempRHReportingFreq)       \n    sensor = 'tempRH'; \nif (msg.batteryReportingFreq)        \n    sensor = 'battery';\n \nswitch (sensor) {\ncase 'airpressure':\n    newMsg = {\n        \"sensorPosition\": parseInt(metadata.ss_pressureSensorPosition),\n        \"schedulingCycle\": parseInt(msg.pressureReportingFreq)\n        };\n        break;\ncase 'dust':\n    newMsg = {\n        \"sensorPosition\": parseInt(metadata.ss_dustSensorPosition),\n        \"schedulingCycle\": parseInt(msg.dustReportingFreq)\n        };\n        break;\ncase 'tempRH':\n    newMsg = {\n        \"sensorPosition\": parseInt(metadata.ss_tempRHSensorPosition),\n        \"schedulingCycle\": parseInt(msg.tempRHReportingFreq)\n        };\n        break;\ncase 'battery':\n    newMsg = {\n        \"sensorPosition\": parseInt(metadata.ss_batterySensorPosition),\n        \"schedulingCycle\": parseInt(msg.batteryReportingFreq)\n        };\n        break;\ndefault:\n    return failure;\n}\n \nnewMetadata = {\n    \"devId\":metadata.originatorName\n};\nreturn {msg: newMsg, metadata: newMetadata, msgType: msgType};"
