      "configuration" : {
        "jsScript" : "var newMsg = {};\nvar newMsgType = 'POST_ATTRIBUTES_REQUEST';\nvar current_date = new Date();\nvar string_date = current_date.getFullYear().toString() + (((current_date.getMonth() + 1).toString().length === 1) ? ('0' + (current_date.getMonth() + 1).toString()) : (current_date.getMonth() + 1).toString()) + ((current_date.getDate().toString().length === 1) ? ('0' + (current_date.getDate()).toString()) : (current_date.getDate()).toString());\n\nif (metadata.perfDate != string_date) {\n    newMsg.perfDate = string_date;\n    metadata.maxPerfDaily = 0;\n    metadata.minPerfDaily = 100;\n}\n\nif (msg.device_performance != undefined) {\n    if (metadata.maxPerfDaily != undefined) {\n        if (msg.device_performance > parseFloat(metadata.maxPerfDaily)){\n            newMsg.maxPerfDaily = msg.device_performance;\n        }\n    } else {\n        newMsg.maxPerfDaily = msg.device_performance;\n    }\n    if (metadata.minPerfDaily != undefined) {\n        if (msg.device_performance < parseFloat(metadata.minPerfDaily)){\n            newMsg.minPerfDaily = msg.device_performance;\n        }\n    } else {\n        newMsg.minPerfDaily = msg.device_performance;\n    }\n}\n\nreturn {msg: newMsg, metadata: metadata, msgType: newMsgType};"
